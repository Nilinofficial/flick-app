{
  "expo": {
    "name": "Flick",
    "slug": "flick",
    "scheme": "flick",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_2.png",
        "backgroundColor": "#000000"
      },
      "package": "com.nilin_dev.flick"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Flick to access your photos and videos."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "728e3d8d-3039-4a86-ba4a-a86a769f50ea"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/167673c6-4a55-4ea0-8902-3cc819f58cb1"
    }
  }
}
